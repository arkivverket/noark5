".\n"
".\n"
"&lt;content&gt;\n"
"  &lt;additionalElements&gt;\n"
"    &lt;additionalElement name=\"archivalPeriod\"&gt;\n"
"      &lt;properties&gt;\n"
"        &lt;property name=\"startDate\"&gt;\n"
"          &lt;value&gt;[ÅÅÅÅ-MM-DD]&lt;/value&gt;\n"
"        &lt;/property&gt;\n"
"        &lt;property name=\"endDate\"&gt;\n"
"          &lt;value&gt;[ÅÅÅÅ-MM-DD]&lt;/value&gt;\n"
"        &lt;/property&gt;\n"
"      &lt;/properties&gt;\n"
"    &lt;/additionalElement&gt;\n"
"  &lt;/additionalElements&gt;\n"
"&lt;/content&gt;\n"
".\n"
".\n"
